{
    "name": "con4gis/core",
    "type": "contao-bundle",
    "description": "Core bundle with functionality needed for every con4gis application.",
    "keywords": ["con4gis", "core", "contao", "c4g", "con4giscore", "con4giscorebundle", "c4gcore"],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Kuestenschmiede",
            "homepage": "https://github.com/kuestenschmiede"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "symfony/framework-bundle": "^3.3",
        "contao/core-bundle": "^4.4.2",
        "doctrine/ORM": "^2.5",
        "doctrine/data-fixtures": "^1.2",
        "doctrine/dbal": "~2.5",
        "menatwork/contao-multicolumnwizard": "^3.3",
        "fortawesome/font-awesome":"^4.7",
        "terminal42/notification_center": "^1.4"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "con4gis\\CoreBundle\\": ""
        },
        "classmap": [
            "Resources/contao/"
        ],
        "exclude-from-classmap": [
            "Resources/contao/config/",
            "Resources/contao/dca/",
            "Resources/contao/languages/",
            "Resources/contao/templates/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "contao-manager-plugin": "con4gis\\CoreBundle\\ContaoManager\\Plugin"
    }
}